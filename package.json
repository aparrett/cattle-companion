{
  "name": "cattle-companion",
  "version": "0.0.1",
  "description": "A web application for managing cattle.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "test": "jest --watchAll --verbose --runInBand",
    "client": "npm run start --prefix client",
    "server": "nodemon index.js",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "engines": {
    "node": "8.11.3",
    "npm": "5.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aparrett/cattle-companion.git"
  },
  "author": "Anthony Parrett",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aparrett/cattle-companion/issues"
  },
  "homepage": "https://github.com/aparrett/cattle-companion#readme",
  "dependencies": {
    "bcrypt": "^3.0.0",
    "concurrently": "^3.6.1",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.3",
    "express-async-errors": "^3.0.0",
    "jest": "^23.5.0",
    "joi": "^13.6.0",
    "joi-objectid": "^2.0.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "mongoose": "^5.2.8",
    "node-cron": "^1.2.1",
    "redux-promise": "^0.6.0",
    "supertest": "^3.1.0",
    "winston": "^2.4.0"
  }
}
